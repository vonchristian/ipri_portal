<div class="md:col-span-6 mt-6 ml-2">
            <h2 class="font-bold text-xl text-gray-900">Case Details</h2>
<div class="text-sm font-medium text-center text-gray-500 border-b border-gray-200 dark:text-gray-400 dark:border-gray-700">
    <ul class="flex flex-wrap -mb-px">
      <li class="mr-2">
          <a href="/documenters/case_details/<%=@case_detail.id %>" class="<%= "inline-block p-4 text-blue-600 rounded-t-lg border-b-2 border-blue-600 active dark:text-blue-500 dark:border-blue-500" if current_page?(documenters_case_detail_path(@case_detail.id)) %>inline-block p-4 rounded-t-lg border-b-2 border-transparent hover:text-gray-600 hover:border-gray-300 dark:hover:text-gray-300">Data</a>
      </li>
      <li class="mr-2">
          <a href="/documenters/case_details/<%=@case_detail.id %>/victims" class="<%= "inline-block p-4 text-blue-600 rounded-t-lg border-b-2 border-blue-600 active dark:text-blue-500 dark:border-blue-500" if current_page?(documenters_case_detail_victims_path(@case_detail.id)) %>inline-block p-4 rounded-t-lg border-b-2 border-transparent hover:text-gray-600 hover:border-gray-300 dark:hover:text-gray-300">Victims</a>
      </li>
      <li class="mr-2">
          <a href="/documenters/individual_victims/<%=@case_detail.id %>/case_details" class="<%= "inline-block p-4 text-blue-600 rounded-t-lg border-b-2 border-blue-600 active dark:text-blue-500 dark:border-blue-500" if current_page?(documenters_individual_victim_case_details_path(@case_detail.id)) %>inline-block p-4 rounded-t-lg border-b-2 border-transparent hover:text-gray-600 hover:border-gray-300 dark:hover:text-gray-300">Human Rights Violations</a>
      </li>
      <li class="mr-2">
          <a href="/documenters/individual_victims/<%=@case_detail.id %>/case_details" class="<%= "inline-block p-4 text-blue-600 rounded-t-lg border-b-2 border-blue-600 active dark:text-blue-500 dark:border-blue-500" if current_page?(documenters_individual_victim_case_details_path(@case_detail.id)) %>inline-block p-4 rounded-t-lg border-b-2 border-transparent hover:text-gray-600 hover:border-gray-300 dark:hover:text-gray-300">Development Projects</a>
      </li>
    </ul>

</div>
  <% if current_page?(documenters_case_detail_path(@case_detail)) %>
      <%= render 'documenters/case_details/partials/profile', case_detail: @case_detail %>
    <% end %>
  <% if current_page?(documenters_case_detail_victims_path(case_detail)) %>
      <%= render 'documenters/case_details/partials/victims', case_detail: case_detail %>
  <% end %>
